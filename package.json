{
  "name": "udacity-storefront-api-project",
  "version": "1.0.0",
  "description": "Udacity Storefront API project",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --config .prettierrc './src/**/*.ts' --write",
    "lint": "eslint './src/**/*.ts'",
    "clean": "rm -r dist",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "jasmine": "jasmine",
    "test": "export APP_ENV=test; db-migrate --env test up && npm run build && npm run jasmine; db-migrate --env test reset",
    "watch": "tsc-watch --noClear -p ./src/tsconfig.json --onSuccess \"npm run start\"",
    "start:dev": "nodemon ./src/index.ts",
    "start": "node ./dist/index.js"
  },
  "author": "Michael Akhnoukh",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jasmine-es6-promise-matchers": "^0.0.33",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.2",
    "jasmine-es6-promise-matchers": "^2.1.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.6.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  }
}
